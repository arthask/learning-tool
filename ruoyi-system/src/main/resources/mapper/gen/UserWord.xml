<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.gencode.mapper.UserWordMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ruoyi.system.gencode.entity.UserWord">
        <id column="id" property="id" />
        <result column="word_id" property="wordId" />
        <result column="word" property="word" />
        <result column="user_id" property="userId" />
        <result column="user_name" property="userName" />
        <result column="period" property="period" />
        <result column="collect_flag" property="collectFlag" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="next_study_time" property="nextStudyTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, word_id, word, user_id, user_name, period, collect_flag, create_time, update_time, next_study_time
    </sql>

</mapper>
